{
  "manifest_version": 3,
  "name": "eBird reviewer utilities",
  "version": "1.3.35",
  "content_scripts": [
    {
      "matches": ["https://review.ebird.org/admin/*"],
      "js": ["utilities.js","qr.js","filterGraph.js","filterExport.js","search.js"]
    }
  ],
  "host_permissions": [
      "*://search.macaulaylibrary.org/*"
  ],
  "description": "Utilities for eBird reviewers.",
  "homepage_url": "https://www.faintlake.com/eBird/extension/ReviewerUtilities/",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "author": "Kent Fiala"
}
