{

  "manifest_version": 2,
  "name": "eBird reviewer utilities",
  "version": "1.0.11",
  "content_scripts": [
    {
      "matches": ["https://review.ebird.org/admin/review.htm*"],
      "js": ["utilities.js"]
    }
  ],

  "description": "Provides conveniences for the ebird review queue/search report page.",
  "homepage_url": "https://www.faintlake.com/eBird/extension/ReviewerUtilities/",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "author": "Kent Fiala",
  "browser_specific_settings": {
     "gecko": {
       "update_url": "https://www.faintlake.com/eBird/extension/updates/updates.json"
     }
  }
}
