{
  "manifest_version": 3,
  "name": "eBird reviewer utilities",
  "version": "1.3.6",
  "content_scripts": [
    {
      "matches": ["https://review.ebird.org/admin/review*","https://review.ebird.org/admin/qr.htm*",
         "https://review.ebird.org/admin/viewFilter.htm*"],
      "js": ["utilities.js","qr.js","filterGraph.js","filterExport.js"]
    }
  ],
  "description": "Utilities for eBird reviewers.",
  "homepage_url": "https://www.faintlake.com/eBird/extension/ReviewerUtilities/",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "author": "Kent Fiala"
}
