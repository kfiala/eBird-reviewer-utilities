{

  "manifest_version": 2,
  "name": "eBird search report downloader",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": ["https://review.ebird.org/admin/review.htm*"],
      "js": ["eBirdReport.js"]
    }
  ],

  "description": "Provides a button for downloading a search report from eBird csv format",
  "homepage_url": "https://www.faintlake.com/eBird/extension/report",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  }
}
